{
  "domain": "nuki_events",
  "name": "Nuki Events",
  "version": "1.3.0-final",
  "documentation": "https://api.nuki.io/",
  "config_flow": true,
  "oauth2": {
    "authorization_url": "https://api.nuki.io/oauth/authorize",
    "token_url": "https://api.nuki.io/oauth/token",
    "scopes": [
      "account",
      "smartlock",
      "smartlock.readOnly",
      "smartlock.log",
      "smartlock.auth",
      "webhook.write",
      "webhook.decentral",
      "offline_access"
    ]
  },
  "iot_class": "cloud_push",
  "integration_type": "device",
  "dependencies": ["application_credentials"],
  "requirements": []
}
